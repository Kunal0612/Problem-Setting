        std::vector<int> expected_4 = {2, -1, 2, 2, -1};
        std::vector<int> result_4 = FindLargestIndex(num_4, vector_4, num_query_4, queries_4);
        assert(result_4 == expected_4);
    // TEST_END

    // TEST
        int num_5 = 4;

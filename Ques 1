        assert(result_8 == expected_8);
    // TEST_END

    // TEST
        int num_9 = 2;
        std::vector<long long> vector_9 = {1, 3};
        int num_query_9 = 5;
        std::vector<std::pair<long long, long long>> queries_9 = {{2, 8}, {1, 3}, {2, 4}, {2, 6}, {2, 2}};
        std::vector<int> expected_9 = {-1, -1, -1, -1, 2};
